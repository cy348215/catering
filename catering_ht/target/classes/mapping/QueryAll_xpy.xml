<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.catering.mapper.QueryAllCount_xpy">
    <select id="findMemCount" resultType="int">
        select
        count(id)
        from
        member
    </select>
    <select id="findMerCount" resultType="int">
        select
        count(id)
        from
        merchant
    </select>
    <select id="findIndCount" resultType="int">
        select
        count(id)
        from
        indent
    </select>
    <select id="findMemuCount" resultType="int">
        select
        count(id)
        from
        memu
    </select>
</mapper>